{
  "name": "react-to-static-or-ssr",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "clean": "rimraf dist/js/* dist/css/* dist/**/*.html dist/**/*.json",
    "dev": "npm-run-all clean dev:*",
    "dev:client": "webpack-dev-server --config webpack/webpack.dev.js --watch",
    "dev:server": "npm run watch:server",
    "build": "npm-run-all build:*",
    "build:client": "webpack --env.production --progress --config webpack/webpack.prod.js",
    "build:server": "webpack --env.production --progress --config webpack/webpack.server.js",
    "watch": "npm-run-all watch:*",
    "watch:client": "webpack --config webpack/webpack.dev.js --watch",
    "watch:server": "webpack --config webpack/webpack.server.js --watch",
    "spa": "npm run clean && npm run watch:client & nodemon --delay 250ms server/dist/spa.js",
    "ssr": "npm run clean && npm-run-all --parallel watch:* & nodemon --delay 250ms server/dist/ssr.js",
    "static": "npm run clean && npm-run-all --parallel watch:* & nodemon --delay 250ms server/dist/static.js",
    "spa:build": "npm-run-all build:*",
    "ssr:build": "npm-run-all build:*",
    "static:build": "npm-run-all build:*",
    "spa:serve": "node server/dist/spa.js",
    "ssr:serve": "node server/dist/ssr.js",
    "static:serve": "node server/dist/static.js"
  },
  "reactSnap": {
    "source": "dist"
  },
  "keywords": [
    "react",
    "static",
    "ssr",
    "typescript",
    "webpack"
  ],
  "author": "Yannick Deubel",
  "license": "MIT",
  "description": "A react starter template for creating single page applications, static html or server side rendering.",
  "homepage": "https://github.com/yandeu/react-to-static-or-ssr#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/yandeu/react-to-static-or-ssr.git"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "html-react-parser": "^0.10.5",
    "js-yaml": "^3.14.0",
    "marked": "^0.7.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-jss": "^10.3.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "serialize-javascript": "^2.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@types/axios": "^0.14.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.7",
    "@types/node": "^12.12.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^5.0.16",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.5",
    "autoprefixer": "^9.8.5",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.6.0",
    "html-webpack-plugin": "^4.3.0",
    "isomorphic-style-loader": "^5.1.0",
    "mini-css-extract-plugin": "^0.8.2",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.32",
    "postcss-loader": "^3.0.0",
    "postcss-modules-values": "^3.0.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^6.2.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.0",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}
